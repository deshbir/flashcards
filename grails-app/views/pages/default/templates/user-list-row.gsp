<tr>
	<td>{{id}}</td>									
	<td>{{firstName}} {{lastName}}</td>									
	<td>{{username}}</td>
	<td>
		<span class="editUser" id="wrapper">
			<a href="#/users/edit/{{id}}"><i class="icon-pencil"></i></a>
		</span>
		<span id="divider">
		|  
		</span>
		<span class="deleteUser" id="wrapper">
			<a href="#/users/delete/{{id}}"><i class="icon-remove"></i></a>
		</span>	
	</td>		
</tr>